programa {
  funcao inicio() {
    
    /* Crie um programa que solicite ao usuário a temperatura em celsius e 
    converta para fahrenheit. Exiba na tela o resultado com uma mensagem amigável.
    Fórmula para cálculo:  F = 1.8 * Celsius + 32
    */

    inteiro Celsius, Fahrenheit
     escreva ("Insira a temperatura en F°: ")

    leia(Celsius)

    Fahrenheit =((Celsius*1.8)+32)

    limpa()
  
    escreva("o resultado em Fahrenheit é : ", Fahrenheit  )










  }
}
