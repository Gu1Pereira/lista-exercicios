programa
{
	funcao inicio()
	{
	/*
	ler dois valores numéricos inteiros(representados pelas variáveis A e B) e apresentar o resultado do quadrado
	da diferença do primeiro valor(variável A) em relação ao segundo valor(variável B)
	*/
		real valor1, valor2, diferenca

		escreva("Insira o primeiro valor: ")
		leia(valor1)

		escreva("Insira o segundo valor: ")
		leia(valor2)

		diferenca = valor1 - valor2

		escreva("A diferença do primeiro valor em relação ao segundo é: ", diferenca)
	}
}
